{
  "kind": "collectionType",
  "collectionName": "accounts",
  "info": {
    "singularName": "account",
    "pluralName": "accounts",
    "displayName": "Account"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullname": {
      "type": "text",
      "required": true
    },
    "photo": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "bio": {
      "type": "text"
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": true,
      "regex": "^[^\\s]+$"
    },
    "links": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::link.link",
      "mappedBy": "account"
    }
  }
}
